')){\r\n            for(var x = 0; x < likes.length; x++){\r\n                if(post._id === likes[x]){\r\n                    localStorage.setItem('likes', likes.splice(x, 1));\r\n                    await axios.post('/remove-like', {id: post._id});\r\n                    setReRender(1)\r\n                    return null;\r\n                }\r\n            }\r\n            localStorage.setItem('likes', likes.push(post._id))\r\n            await axios.post('/add-like', {id: post._id});\r\n            setReRender(1)\r\n            return null;\r\n        }else{\r\n            localStorage.setItem('likes', [post._id])\r\n            await axios.post('/add-like', {id: post._id});\r\n            return null;\r\n        }\r\n    }\r\n\r\n    if(post){\r\n        return(\r\n            <div>\r\n                <br />\r\n                <br />\r\n                <h1 className=\"post-view-title\">{post.title}</h1>\r\n                <img className=\"post-view-image mx-auto d-block\" src={post.picture} />\r\n                <span className=\"author\">من {post.author || \"مجهول\"}</span>\r\n                <p className=\"post-view-content\">{post.content}</p>\r\n                <hr />\r\n                <br />\r\n                <br />\r\n                <form>\r\n                    <input className=\"form-control\" onChange={commentChanged} placeholder=\"اترك تعليق!\" />\r\n                    <button className=\"btn btn-primary\" onClick={submit}>إرسال</button>\r\n                </form>\r\n                <br />\r\n\r\n                <h3>التعليقات:</h3>\r\n                {comments}\r\n            </div>\r\n        );\r\n    }\r\n    else{\r\n        return(\r\n            <div>\r\n                <br />\r\n                <br />\r\n                <h1>لم يتم إيجاد المنشور الذي تبحث عنه !</h1>\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nexport default PostView;","import React, {useState} from 'react';\r\nimport {useHistory} from 'react-router-dom';\r\nimpor